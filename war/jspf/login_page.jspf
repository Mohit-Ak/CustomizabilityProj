    <body class=" login">
        <!-- BEGIN : LOGIN PAGE-->
        <div class="user-login-5 login-page-wrap page-content-container" id="custom_login_page">
            <div class="row bs-reset">
                <div class="col-md-12 login-container bs-reset">
<!--                     <img class="login-logo login-6" src="../assets/pages/img/login/login-invert.png" /> -->
                    <div class="login-content">
<!--                         <h1>Customizability Project Login</h1> -->
                        <p>Please Enter Your ID number and Password provided to you earlier by  the researchers.</p>
                        <form action="javascript:;" class="login-form" method="post">
                            <div class="alert alert-danger display-hide">
                                <button class="close" data-close="alert"></button>
                                <span>Enter your ID Number and password. </span>
                            </div>
                            <div class="row">
                                <div class="col-xs-6">
                                    <input class="form-control form-control-solid placeholder-no-fix form-group" type="text" autocomplete="off" placeholder="User Id" id="customUserId" name="username" required/> </div>
                                <div class="col-xs-6">
                                    <input class="form-control form-control-solid placeholder-no-fix form-group" type="password" autocomplete="off" placeholder="Password" id="customPassword" name="password" required/> </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-4">
                                  
                                </div>
                                <div class="col-sm-8 text-right">
                                    <div class="forgot-password">
                                        <a href="javascript:;" id="forget-password" class="forget-password">Forgot Password?</a>
                                    </div>
                                    <button class="btn blue" type="submit" id="customLoginSubmit">Sign In</button>
                                </div>
                            </div>
                        </form>
                        <!-- BEGIN FORGOT PASSWORD FORM -->
                        <form class="forget-form" action="javascript:;" method="post">
                            <h3>Forgot Password ?</h3>
                            <p> Contact Prof.Ivan Dylko for the password. </p>
                            <div class="form-group hidden">
                                <input class="form-control placeholder-no-fix" type="text" autocomplete="off" placeholder="Email" name="email" /> </div>
                            <div class="form-actions">
                                <button type="button" id="back-btn" class="btn blue btn-outline">Back</button>
                            </div>
                        </form>
                        <!-- END FORGOT PASSWORD FORM -->
                    </div>
                    <div class="login-footer">
                        <div class="row bs-reset">
                            
                            <div class="col-xs-12 bs-reset">
                                <div class="login-copyright text-center">
                                    <p>Copyright &copy; University at Buffalo 2016</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
<!--                 <div class="col-md-6 bs-reset"> -->
<!--                     <div class="login-bg"> </div> -->
<!--                 </div> -->
            </div>
        </div>
        <!-- END : LOGIN PAGE-->
